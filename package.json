{
  "name": "gear-size-calculator",
  "version": "1.0.1",
  "description": "Create gear size data for bike ratio comparison",
  "author": "steve.o.cassels@gmail.com",
  "main": "./lib/calculator.js",
  "scripts": {
    "preversion": "npm test",
    "prepublish": "coffee -o lib/ -c src/calculator.coffee",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "minor-release": "npm version minor && npm publish && git push --follow-tags",
    "major-release": "npm version major && npm publish && git push --follow-tags",
    "test:w": "cross-env NODE_ENV=test nyc mocha --watch --recursive --reporter landing 'src/**/*spec.js'",
    "test": "./node_modules/.bin/nodeunit tests && npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test nyc mocha --recursive --reporter landing 'src/**/*spec.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Steve-O-Cassels/gear-size-calculator.git"
  },
  "keywords": [
    "bike",
    "gear-size",
    "gear-ratio"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "lodash": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coffee-script": "^1.11.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.12.2",
    "eslint-config-google": "^0.7.1",
    "mocha": "^3.2.0",
    "mockery": "^2.0.0",
    "nodeunit": "^0.9.1",
    "nyc": "^10.0.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0"
  }
}
